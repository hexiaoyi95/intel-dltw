{
    "backend" : {
        "python_path" : "dl-frameworks/dl_framework-intel_caffe/python",
        "class_path" : "backends.caffe_backend.CaffeBackend",
        "engine" : "MKL2017"
    },
    "application" : "applications.convergence",
    "model" : {
        "topology" : "dl-models/bvlc_alexnet/solver.prototxt" ,
